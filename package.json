{
  "name": "Gymble",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "ios-d": "react-native run-ios --device \"Jugno\"",
    "ios-u": "react-native run-ios --device \"Usman iphone\"",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "android-staging": "react-native run-android --variant=buildStagingRelease",
    "android-release": "react-native run-android --variant=buildProdRelease",
    "ios-release": "export PROJECT_ENV=rn && react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios",
    "bundle:ios": " react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios",
    "bundle:android": "react-native bundle --platform android --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --dev false --reset-cache --assets-dest android/app/src/main/res/",
    "postinstall": "yarn run bundle:ios",
    "android:asseble": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/",
    "clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all && npm cache clean --force",
    "android:clean": "cd android && ./gradlew clean",
    "android:releaseBuild": "cd android && ./gradlew assembleRelease",
    "android:releaseBundle": "cd android && ./gradlew/bundleRelease",
    "android:release": "react-native run-android --variant=release",
    "android:publish": "cd android && ./gradlew clean && fastlane android beta",
    "pod-install": "cd ios && pod install",
    "setup-sentry": "npx @sentry/wizard",
    "link": "npx react-native link"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.17.3",
    "@react-native-community/cli-platform-android": "^7.0.1",
    "@react-native-masked-view/masked-view": "^0.2.6",
    "@react-native-picker/picker": "^2.4.1",
    "@react-navigation/bottom-tabs": "^6.3.1",
    "@react-navigation/native": "^6.0.10",
    "@react-navigation/stack": "^6.2.1",
    "@types/react-native-vector-icons": "^6.4.10",
    "@yoonzm/react-native-alphabet-flat-list": "^1.3.0",
    "axios": "^0.26.1",
    "prop-types": "^15.8.1",
    "radio-buttons-react-native": "^1.0.4",
    "react": "18.0.0",
    "react-native": "0.68.1",
    "react-native-alphabetflatlist": "^1.0.5",
    "react-native-app-intro-slider": "^4.0.4",
    "react-native-chart-kit": "^6.12.0",
    "react-native-flatlist-alphabet": "^1.1.2",
    "react-native-gesture-handler": "^2.4.0",
    "react-native-image-crop-picker": "^0.37.3",
    "react-native-image-picker": "^4.7.3",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-keyboard-spacer": "^0.4.1",
    "react-native-modal": "^13.0.1",
    "react-native-picker-select": "^8.0.4",
    "react-native-raw-bottom-sheet": "^2.2.0",
    "react-native-safe-area-context": "^4.2.4",
    "react-native-screens": "^3.13.1",
    "react-native-section-alphabet-list": "^2.1.0",
    "react-native-simple-toast": "^1.1.3",
    "react-native-splash-screen": "^3.3.0",
    "react-native-svg": "^12.3.0",
    "react-native-switch-selector": "^2.2.0",
    "react-native-universal-attach-picker": "^1.0.3",
    "react-native-vector-icons": "^9.1.0",
    "rn-keyboard-sticky-view": "^1.0.4",
    "toggle-switch-react-native": "^3.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/runtime": "^7.17.9",
    "@react-native-community/eslint-config": "^3.0.1",
    "@types/jest": "^27.4.1",
    "@types/react": "^18.0.5",
    "@types/react-native": "^0.67.4",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^27.5.1",
    "eslint": "8.13.0",
    "jest": "^27.5.1",
    "metro": "^0.70.1",
    "metro-config": "^0.70.1",
    "metro-react-native-babel-preset": "^0.70.1",
    "react-test-renderer": "18.0.0",
    "typescript": "^4.6.3"
  },
  "jest": {
    "preset": "react-native"
  }
}
